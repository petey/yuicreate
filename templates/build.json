{
    "name"    : "{{modulename}}",
    "type"    : "js",
    "version" : "1.0.0",
    "skip"    : {
        "clean"    : false,
        "register" : {{skipRegister}},
        "lint"     : false,
        "logger"   : false
    },
    "sourceDir"   : "js",
    "sourceFiles" : [
        "{{modulename}}.js"
    ],
    "buildDir"  : "../../build/{{modulename}}",
    "assetsDir" : "assets",
    "tools"     : {
        "jslint"  : {
            "jslint option" : null
        },
        "csslint" : {
            "csslint option" : null
        }
    },
    "details" : {
        "use"        : {{use}},
        "supersedes" : {{supercedes}},
        "requires"   : {{requires}},
        "optional"   : {{optional}},
        "after"      : null,
        "after_map"  : {},
        "skinnable"  : {{skinnable}}
    }
}
